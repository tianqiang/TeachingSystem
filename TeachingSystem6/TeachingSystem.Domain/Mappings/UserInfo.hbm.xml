<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
   assembly="TeachingSystem.Domain"
   namespace="TeachingSystem.Domain.Entities">

  <class name ="UserData" table="user" lazy="false">
    <id name="Account" column="Account">
      <generator class ="native"/>
    </id>
    <property name ="Account"/>
    <property name ="Role"/>
    <property name ="Name"/>
    <property name ="NickName"/>
    <property name ="Email"/>
    <property name ="Password"/>
    <property name ="Sex"/>
    <property name ="Major"/>
    <property name ="College"/>
  </class>
  <class name ="Course" table="course" lazy="false">
    <id name="Number" column="Number">
      <generator class ="native"/>
    </id>
    <property name ="Name"/>
    <property name ="Teacher"/>
    <property name ="Account"/>
    <property name ="Place"/>
    <property name ="Number"/>
    <property name ="Content"/>
    <property name ="PreCourse"/>
    <property name ="Majoy"/>
    <property name ="MaxNum"/>
    <property name ="Score"/>
    <property name ="Type"/>
    <property name ="TestType"/>
  </class>
  <class name ="Time" table="time" lazy="false">
    <id name="Number" column="Number">
      <generator class ="native"/>
    </id>
    <property name ="Number"/>
    <property name ="Day"/>
    <property name ="Week"/>
    <property name ="TimeSpan"/>
  </class>
  <class name ="StudentCourse" table="studentcourse" lazy="false">
    <id name="KeyNum" column="KeyNum">
      <generator class ="native"/>
    </id>
    <property name ="KeyNum"/>
    <property name ="Account"/>
    <property name ="CourseName"/>
    <property name ="CommentToTeacher"/>
    <property name ="CommentToStudent"/>
    <property name ="Number"/>
    <property name ="Score"/>
    <property name ="State"/>
  </class>
</hibernate-mapping>