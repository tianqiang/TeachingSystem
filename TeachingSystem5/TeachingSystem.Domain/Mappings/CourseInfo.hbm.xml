<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
   assembly="TeachingSystem.Domain"
   namespace="TeachingSystem.Domain.Entities">

	<class name ="Course" table="course" lazy="false">
		<id name="Number" column="Number">
			<generator class ="native"/>
		</id>
		<property name ="Name"/>
		<property name ="Teacher"/>
		<property name ="Place"/>
		<property name ="Number"/>
     <property name ="Content"/>
		<property name ="Time"/>
		<property name ="PreCourse"/>
		<property name ="Majoy"/>
		<property name ="MaxNum"/>
		<property name ="Score"/>
		<property name ="Type"/>
		<property name ="TestType"/>
	</class>
</hibernate-mapping>